{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@dal": ["src/lib/dal"],
      "@dal/*": ["src/lib/dal/*"],
      "@jws": ["src/lib/jws"],
      "@jws/*": ["src/lib/jws/*"]
    }
  }
}
